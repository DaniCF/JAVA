package com.daniel;

public class ElQueDecide {

	public static void main(String[] args) {

		 int a,b,c;
		 
		 a = 83;
		 b = 45;
		 c = 122;
		 
		 if (a > b) {
			 if (a > c) {
				 System.out.println(a);
			 }
			 else {
				System.out.println(c);
			 }
			
		 }
		 else {
			 System.out.println(b);
		 }

	}

}
